{
  "tags": {
    "allowUnknownTags": [],
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "presets": [
      "stage-2",
      "react"
    ],
    "babelrc": false
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./new_site/",
    "template": "jsdoc/templates/microcosm",
    "tutorials": "jsdoc/tutorials",
    "recurse": true,
    "readme": "./jsdoc/README.md"
  }
}
